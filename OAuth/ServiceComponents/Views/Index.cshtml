@{
    Layout = "_Layout";
}

@section Head
{

}
<form action="/Authorization/Login" method="post">
    <input type="hidden" name="OAuthToken" value="@Model.Token" />
    <div>
        <label>Email</label>
        <input type="text" name="userName" />
    </div>
    <div>
        <label>Password</label>
        <input type="password" name="password" />
    </div>
    <input type="submit" />
</form>
